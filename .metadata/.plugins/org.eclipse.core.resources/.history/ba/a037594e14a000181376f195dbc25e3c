import java.util.Scanner;

public class Stringstr_Main
{

	public static void main(String[] args) {
		StringStr str = new StringStr();
		str.setS1("Hello");
		boolean result = str.startHi(str.getS1());
		System.out.println("The result of the string starts with hi then the condition is " + result);
		
		
	}
